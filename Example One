#include <iostream>

using namespace std;

int main()
{


//delclare variables

int mult[5][8];
int i;
int w;


//filling the arrays

        
     for(i=0;i<5;i++) {
    
    for(w=0;w<8;w++) {
        
        //mutiplying i and w to fill the sport
        mult[i][w]=(i+1)*(w+1);
        
        
    }
    
     }
     
     //print the line
     for(i=0;i<5;i++) {
    
    //putting a linr
    cout<<"\n";
    
    for(w=0;w<8;w++) {
        
        //mutiplying i and w to fill the sport
        cout<<"\t"<<mult[i][w];
        
        
        
    }
    
    
    
}

}
    
 
