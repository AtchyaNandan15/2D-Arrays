#include <iostream>
#include<math.h>

using namespace std;

int main()
{


//delclare variables

int mult[7][7];
int i;
int w;
int num1;
int num2;


        
     for(i=0;i<7;i++) {
    
    
     for(w=0;w<7;w++) {
        
     
        mult[i][w]=pow(i+2, w);
        
        
        
    }
    
     }

     cout<<"\t0\t1\t2\t3\t4\t5\t6";
     

     for(i=0;i<7;i++) {
    
   
    cout<<"\n";
    cout<<i+2<<"";
    
    for(w=0;w<7;w++) {
        
        
        
        cout<<"\t"<<mult[i][w];
        
        
        
    }
     }
    
    cout<<"\nEnter a number";
    cin>>num1;
    
    cout<<"\nEnter a number";
    cin>>num2;
    
  cout<<mult[num1- 2][num2];
    
    
    

    
    


}
    
 
