#include <iostream>

using namespace std;

int main()
{
   
   
   int table[6][8];
   int i;
   int w;
   
    for(i=0;i<6;i++) {
    for(w=0;w<8;w++) {
        
     table[i][w]=(i+1)*(w+1);
    }
   
       
    }
   
   
    for(i=0;i<6;i++) {
    for(w=0;w<8;w++) {
 
 
    }
       cout<<table[i][w]<<"\t";
       
    }
   
   
   
   
   
   
    
   
}
