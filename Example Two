#include <iostream>

using namespace std;

int main()
{


//delclare variables

int mult[5][8];
int i;
int w;


//filling the arrays

        
     for(i=0;i<5;i++) {
    
    for(w=0;w<8;w++) {
        
        //mutiplying i and w to fill the sport
        mult[i][w]=(i+1)*(w+1);
        
        
    }
    
     }
     
     //title and numbers outputted
     cout<<"\t This is my multiplication table\n";
     cout<<"1\t2\t|3\t|4\t|5\t|6\t|7\t|8\n\n";
     cout<<"-------------------------------------------------------------------\n";
     
    //print the line
    for(i=0;i<5;i++) {
    
    //putting a linr
    cout<<"\n";
    cout<<i+1<<"\t|";
    
    for(w=0;w<8;w++) {
        
    //mutiplying i and w to fill the sport
    cout<<mult[i][w]<<"\t|";
    
}
    
    cout<<"\n--------------------------------------------------------------------\n";

    
        

    
}

}
    
 
