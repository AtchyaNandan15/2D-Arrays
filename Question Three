#include <iostream>
#include <string>

using namespace std;

int main()
{
  
  int siblings;
  int cousins;
  int friends;
  
  int students[5][3];
  int i;
  int w;
  int total=0;
  
  for(i=0;i<5;i++) {
      for(w=0;w<3;w++) {
          students[i][w];
          
          
           cout<<" Student " <<i+1<< " Enter the number of siblings, cousins and friends you have ["<<i<<"]["<<w<<"] ";
           cin>>students[i][w];
           
        
          
      }
      cout<<"\n";
      
  }
      
      //cout<<students[4][2];
      
      total=  total + students[i][1];
      
      cout<<"\n"<<total<<"\n";
      
      
  
  
}
  
  
  
  
  
