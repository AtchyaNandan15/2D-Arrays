#include <iostream>
#include <string>

using namespace std;

int main()
{
  
  int siblings;
  int cousins;
  int friends;
  
  int students[5][3];
  int i;
  int w;
  int total=0;
  
  for(i=0;i<5;i++) {
      for(w=0;w<3;w++) {
         
          students[i][w];
            
      }
      
  } 
  
  for(i=0;i<5;i++) {
                
     
          
           cout<<" Student " <<i+1<< " Enter the number of siblings you have\n";
           cin>>students[i][1];
           
           cout<<" Student " <<i+1<< " Enter the number of cousins you have\n";
           cin>>students[i][2];
           
           cout<<" Student " <<i+1<< " Enter the number of friends you have\n";
           cin>>students[i][3]; 
           
          
  }
        
  
  cout<<" The total number of siblings from all five students is ";   
  cout<<"\n"<<students[0][1]+students[1][1]+students[2][1]+students[3][1]+students[4][1]<<"";  
  
  cout<<" The total number of cousins from all five students is ";   
  cout<<"\n"<<students[0][2]+students[1][2]+students[2][2]+students[3][2]+students[4][2]<<"";  
  
  cout<<" The total number of friends from all five students is ";   
  cout<<"\n"<<students[0][3]+students[1][3]+students[2][3]+students[3][3]+students[4][3]<<"";  
  

   
  
  }
      


       
  
  
  
  
